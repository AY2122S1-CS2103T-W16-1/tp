<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<TabPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <tabs>
      <Tab closable="false" text="Client Information">
         <content>
            <VBox fx:id="clientInfoTab">
                <ListView fx:id="clientInfoList" VBox.vgrow="ALWAYS" />
            </VBox>
         </content>
      </Tab>
      <Tab closable="false" text="Financial Information" />
      <Tab closable="false" text="Notes">
         <content>
         </content>
         <content>
            <TextArea fx:id="notesTextArea" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </Tab>
   </tabs>
</TabPane>

